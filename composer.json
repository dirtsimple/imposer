{
    "name": "dirtsimple/imposer",
    "type": "library",
    "license": "MIT",
    "description": "Modular Configuration Management for Wordpress",
    "homepage": "https://github.com/dirtsimple/imposer/",
    "authors": [
        {
            "name": "PJ Eby",
            "homepage": "https://github.com/pjeby"
        }
    ],
    "keywords": [
        "wordpress",
        "composer",
        "literate-devops",
        "configuration-management",
        "wp",
        "markdown"
    ],
    "support": {
        "issues": "https://github.com/dirtsimple/imposer/issues"
    },
    "require": {
        "php": ">=5.6",
        "wp-cli/eval-command": "^1.0.6|dev-master"
    },
    "require-dev": {
        "ext-yaml": "*"
     },
    "suggest": {
        "ext-yaml": "For YAML processing if you don't have yaml2json or PyYAML"
    },
    "bin": [
        "bin/imposer"
    ]
}
